{
    "manifest_version": 2,
    "name": "Latex Equation Generator",
    "version": "1.0",
    "description": "Generates latex equation images.",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["storage"],
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "index.html"
    }
}
