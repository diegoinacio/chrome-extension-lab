{
    "manifest_version": 2,
    "name": "WhatsApp Links",
    "version": "1.0",
    "description": "Generates WhatsApp links.",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "browser_action": {
        "default_icon": "icon16.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "tabs"
    ]
}
